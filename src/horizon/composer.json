{
    "name": "hypervel/horizon",
    "description": "The horizon package for Hypervel.",
    "license": "MIT",
    "keywords": [
        "php",
        "swoole",
        "hypervel",
        "horizon"
    ],
    "support": {
        "issues": "https://github.com/hypervel/components/issues",
        "source": "https://github.com/hypervel/components"
    },
    "authors": [
        {
            "name": "Albert Chen",
            "email": "albert@hypervel.org"
        }
    ],
    "require": {
        "php": "^8.2",
        "hyperf/context": "~3.1.0",
        "nesbot/carbon": "^2.72.6",
        "hypervel/support": "^0.3",
        "hypervel/cache": "^0.3",
        "hypervel/core": "^0.3",
        "hypervel/console": "^0.3",
        "hypervel/event": "^0.3",
        "hypervel/queue": "^0.3"
    },
    "autoload": {
        "psr-4": {
            "Hypervel\\Horizon\\": "src/"
        }
    },
    "extra": {
        "branch-alias": {
            "dev-main": "0.3-dev"
        },
        "hypervel": {
            "providers": [
                "Hypervel\\Horizon\\HorizonServiceProvider"
            ]
        }
    },
    "suggest": {
        "hypervel/queue": "Required to use closures when chaining jobs."
    },
    "config": {
        "sort-packages": true
    },
    "minimum-stability": "dev"
}
